{% extends "layout.html.twig" %}

{% block title %}{{ test.method }} ({{ test.getTestCaseEvent().class }}) &minus; {{ parent() }}{% endblock %}

{% block content %}
    <ul class="breadcrumb">
        <li><a href="./">Report</a> <span class="divider">/</span></li>
        <li><a href="#">{{ test.getTestCaseEvent().class }}</a> <span class="divider">/</span></li>
        <li class="active">{{ test.method }}</li>
    </ul>
    <div class="progress">
        <div class="bar bar-success" style="width: 70%;" title="Success: 2/3"></div>
        <div class="bar bar-warning" style="width: 10%;" title="Skipped: 0/3"></div>
        <div class="bar bar-danger" style="width: 20%;" title="Failed: 1/3"></div>
    </div>
    <table class="table table-striped table-bordered">
        <thead>
        <tr>
            <th>Head</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>Body</td>
        </tr>
        </tbody>
    </table>
{% endblock %}