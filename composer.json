{
    "name": "komex/unteist",
    "description": "Unit test framework",
    "keywords": [
        "testing", "test", "unit test", "phpunit", "framework"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrey Kolchenko",
            "email": "andrey@kolchenko.me",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "andrey@kolchenko.me"
    },
    "require": {
        "php": ">=5.4.0",
        "ext-SPL": "*",
        "ext-Reflection": "*",
        "symfony/yaml": "2.3.*",
        "symfony/finder": "2.3.*",
        "symfony/config": "2.3.*",
        "symfony/console": "2.3.*",
        "symfony/filesystem": "2.3.*",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/dependency-injection": "2.3.*",
        "monolog/monolog": "1.6.*",
        "sebastian/diff": "1.0.*",
        "twig/twig": "1.13.*",
        "twitter/bootstrap": "2.3.2",
        "leafo/lessphp": "~0.3.9",
        "jquery/jquery": "2.0.*"
    },
    "suggest": {
        "ext-pcntl": "Required for run in separate processes."
    },
    "autoload": {
        "psr-0": {
            "Unteist\\": "src"
        }
    },
    "bin": [
        "bin/unteist.php"
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.0.3",
                "dist": {
                    "url": "http://code.jquery.com/jquery-2.0.3.min.js",
                    "type": "file"
                }
            }
        }
    ]
}
