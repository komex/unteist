{
    "name": "komex/unteist",
    "description": "Unit test framework",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Andrey Kolchenko",
            "email": "andrey@kolchenko.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-SPL": "*",
        "ext-Reflection": "*",
        "symfony/finder": "~2.3.2",
        "symfony/console": "~2.3.2",
        "symfony/filesystem": "~2.3.2",
        "symfony/event-dispatcher": "~2.3.2",
        "symfony/dependency-injection": "~2.3.2",
        "monolog/monolog": "~1.5.0",
        "sebastian/diff": "1.0.*",
        "twig/twig": "~1.13.1",
        "twitter/bootstrap": "2.3.2",
        "leafo/lessphp": "~0.3.9",
        "jquery/jquery": "2.0.3"
    },
    "autoload": {
        "psr-0": {
            "Unteist\\": "src"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.0.3",
                "dist": {
                    "url": "http://code.jquery.com/jquery-2.0.3.min.js",
                    "type": "file"
                }
            }
        }
    ]
}
